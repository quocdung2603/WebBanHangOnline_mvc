@{
    ViewBag.Title = "Đặt lại mật khẩu";
}
<link rel="stylesheet" type="text/css" href="~/Content/assets/styles/main_styles.css">

<style>
    .news_section_container {
        margin-top: 180px;
    }
</style>

<div class="container news_section_container">
    <div class="row">
        <div class="col-md-12">
            <hgroup class="title">
                <h2>@ViewBag.Title.</h2>
            </hgroup>
            <div>
                <p>
                   Hãy kiểm tra Email để nhận đường dẫn đặt lại mật khẩu.
                </p>
            </div>
        </div>
    </div>
</div>


@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}



